<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.Transparent" parent="AppTheme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="MainActivityTheme" parent="AppTheme">
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <!--rect corner imageView-->
    <style name="roundedCornerImageStyle_8b">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/dp_8</item>
    </style>
    <style name="roundedCornerImageStyle_10b">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/dp_10</item>
    </style>
    <!--round corner imageView-->
    <style name="circleCornerImageStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <style name="TabLayoutStyle">
        <item name="android:textSize">@dimen/sp_16</item>
    </style>
    <style name="clubToolbar" parent="Widget.AppCompat.Toolbar">
        <item name="contentInsetStart">0dp</item>
    </style>
    <style name="FabStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">16dp</item>
        <item name="borderWidth">0dp</item>
        <item name="elevation">6dp</item>
        <item name="pressedTranslationZ">12dp</item>
        <item name="rippleColor">@android:color/white</item>
    </style>
</resources>